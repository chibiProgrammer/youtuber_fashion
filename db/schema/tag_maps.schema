create_table "tag_maps", force: :cascade do |t|
    t.bigint "post_id",  comment: "投稿ID"
    t.bigint "tag_id", comment: "タグID"
    t.datetime "created_at", precision: 6, null: false
    t.datetime "updated_at", precision: 6, null: false
end
